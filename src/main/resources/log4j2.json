{
  "configuration": {
    "status": "WARN",
    "properties": {
      "property": {
        "name": "filename",
        "value": "log/log.log"
      }
    },
    "appenders": {
      "file": {
        "type": "file",
        "name": "file",
        "fileName": "${filename}",
        "PatternLayout": {
          "pattern": "%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"
        }
      }
    },
    "loggers": {
      "logger": {
        "name": "org.fundacionjala.pivotaltracker.api",
        "level": "ALL",
        "additivity": "false",
        "AppenderRef": {
          "ref": "file"
        }
      },
      "root": {
        "level": "ALL",
        "AppenderRef": {
          "ref": "file"
        }
      }
    }
  }
}
